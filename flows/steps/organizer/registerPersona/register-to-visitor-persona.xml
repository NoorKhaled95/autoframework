<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Register in 'Visitor' via landing page">
    <listeners>
        <listener class-name="configs.listeners.Listener"/>
        <listener class-name="configs.listeners.TimeoutExceptionListener"/>
    </listeners>
    <test name="Register through Landing Page in 'Visitor' form">

        <classes>
            <class name="tests.organizer.registration.landingPageForm.UITest"/>
            <class name="tests.organizer.registration.landingPageForm.RequiredFieldsValidationTest"/>
            <class name="tests.organizer.registration.landingPageForm.VisitorRegistrationTest"/>
        </classes>
    </test>
</suite>