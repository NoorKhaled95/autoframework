<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Onsite Register For VIP Persona - Auto Print">
    <listeners>
        <listener class-name="configs.listeners.Listener"/>
        <listener class-name="configs.listeners.TimeoutExceptionListener"/>
    </listeners>
    <test name="Onsite VIP Registration With Auto-Print">
        <groups>
            <run>
                <include name="auto-print=ON"/>
            </run>
        </groups>
        <classes>
            <class name="tests.crew.onsiteRegistration.singleForm.CrewVipRegistrationTest"/>
        </classes>
    </test>
</suite>